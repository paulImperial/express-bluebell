{
	"name": "express-bluebell",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "NODE_ENV=development parcel index.html --open",
		"build-css": "node-sass --include-path --compress scss src/sass/main.scss -o public/css && npm run dev",
		"watch-css": "nodemon -e --watch view --watch src -x \"npm run build-css\"",
		"next": "next && npm run start",
		"build": "NODE_ENV=production parcel build dist/index.html --public-url ./",
		"build-next": "next build && next export",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/paulImperial/express-bluebell.git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test",
			"pre-push": "npm test"
		}
	},
	"keywords": [],
	"author": "Paul Mason",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/paulImperial/express-bluebell/issues"
	},
	"homepage": "https://github.com/paulImperial/express-bluebell#readme",
	"dependencies": {
		"babel-preset-react": "^6.24.1",
		"express": "^4.16.3",
		"express-handlebars": "^3.0.0",
		"jest-cli": "^24.7.1",
		"next": "^8.0.1",
		"next-server": "^8.0.1",
		"node-sass": "^4.9.2",
		"nodemon": "^1.18.3",
		"parcel-bundler": "^1.12.3",
		"react": "^16.8.6",
		"react-dom": "^16.8.6"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.4.2",
		"@babel/preset-es2015": "^7.0.0-beta.53",
		"@babel/preset-react": "^7.0.0",
		"babel-cli": "x",
		"babel-preset-env": "^1.7.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.13.0",
		"husky": "^2.3.0",
		"parcel-plugin-imagemin": "^2.1.3",
		"prettier": "^1.17.1"
	}
}
